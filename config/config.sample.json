{
  "database": {
    "host": "$HABIDAT_DK_DB_HOST",
    "dialect": "mysql",
    "username": "$MARIADB_USER",
    "password": "$MARIADB_PASSWORD",
    "database": "$MARIADB_DATABASE"
  },
  "auth": {
    "admin": {
      "method": ["$HABIDAT_DK_AUTH_ADMIN_METHOD"],
      "ldap": {
        "server": {
          "url": "$HABIDAT_DK_AUTH_ADMIN_LDAP_URI",
          "bindDN": "$HABIDAT_DK_AUTH_ADMIN_LDAP_BINDDN",
          "bindCredentials": "$HABIDAT_DK_AUTH_ADMIN_LDAP_PASSWORD",
          "searchBase": "$HABIDAT_DK_AUTH_ADMIN_LDAP_BASE",
          "searchFilter": "$HABIDAT_DK_AUTH_ADMIN_LDAP_SEARCHFILTER"
        },
        "passReqToCallback": true,
        "usernameField": "userid"
      },
      "oidc": {
        "label": "$HABIDAT_DK_AUTH_ADMIN_OIDC_LABEL",
        "issuer": "$HABIDAT_DK_AUTH_ADMIN_OIDC_ISSUER",
        "authorizationURL": "$HABIDAT_DK_AUTH_ADMIN_OIDC_AUTH_URL",
        "tokenURL": "$HABIDAT_DK_AUTH_ADMIN_OIDC_TOKEN_URL",
        "userInfoURL": "$HABIDAT_DK_AUTH_ADMIN_OIDC_USERINFO_URL",
        "clientID": "$HABIDAT_DK_AUTH_ADMIN_OIDC_CLIENT_ID",
        "clientSecret": "$HABIDAT_DK_AUTH_ADMIN_OIDC_CLIENT_SECRET",
        "callbackURL": "$HABIDAT_DK_AUTH_ADMIN_OIDC_CALLBACK_URL"
      }
    },
    "user": {
      "method": ["$HABIDAT_DK_AUTH_USER_METHOD"],
      "oidc": {
        "label": "$HABIDAT_DK_AUTH_USER_OIDC_LABEL",
        "issuer": "$HABIDAT_DK_AUTH_USER_OIDC_ISSUER",
        "authorizationURL": "$HABIDAT_DK_AUTH_USER_OIDC_AUTH_URL",
        "tokenURL": "$HABIDAT_DK_AUTH_USER_OIDC_TOKEN_URL",
        "userInfoURL": "$HABIDAT_DK_AUTH_USER_OIDC_USERINFO_URL",
        "clientID": "$HABIDAT_DK_AUTH_USER_OIDC_CLIENT_ID",
        "clientSecret": "$HABIDAT_DK_AUTH_USER_OIDC_CLIENT_SECRET",
        "callbackURL": "$HABIDAT_DK_AUTH_USER_OIDC_CALLBACK_URL"
      }
    }
  },
  "site": {
    "http": "true",
    "https": "$HABIDAT_DK_HTTPS",
    "sslcert": "$HABIDAT_DK_SSL_CERT",
    "sslkey": "$HABIDAT_DK_SSL_KEY",
    "porthttps": "$HABIDAT_DK_PORT_HTTPS",
    "porthttp": "$HABIDAT_DK_PORT_HTTP",
    "reverseproxy": "false"
  }
}
